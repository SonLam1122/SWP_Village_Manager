    <script>
        const LOCATIONIQ_API_KEY = "pk.134d02303b552ffc26e2277992e7e810";
        // 🔍 Hàm lấy tọa độ từ địa chỉ (dùng Nominatim - OpenStreetMap)
        async function getCoordinatesFromAddress(address) {
            const url = `https://us1.locationiq.com/v1/search.php?key=${LOCATIONIQ_API_KEY}&q=${encodeURIComponent(address)}&format=json`;
            const response = await fetch(url);
            const data = await response.json();

            if (data.length === 0) throw new Error("Không tìm thấy tọa độ từ địa chỉ.");

            return {
                latitude: parseFloat(data[0].lat),
                longitude: parseFloat(data[0].lon)
            };
        }

        // 📦 Xử lý khi người dùng submit form
        document.getElementById("address-form").addEventListener("submit", async function (e) {
            e.preventDefault();

            try {
                // Lấy dữ liệu người dùng nhập
                const userId = parseInt(document.getElementById("userId").value);
                const label = document.getElementById("fname").value;
                const address = document.getElementById("address").value;

                if (!label || !address) {
                    alert("Vui lòng nhập đầy đủ tên và địa chỉ.");
                    return;
                }

                // 🔄 Gọi API để lấy tọa độ từ địa chỉ
                const coords = await getCoordinatesFromAddress(address);

                // Cập nhật giá trị vào input ẩn
                document.getElementById("latitude").value = coords.latitude;
                document.getElementById("longitude").value = coords.longitude;

                // Chuẩn bị dữ liệu gửi đi
                const data = {
                    UserId: userId,
                    Label: label,
                    Address: address,
                    Latitude: coords.latitude,
                    Longitude: coords.longitude
                };

                console.log("Gửi dữ liệu:", data);

                // 📨 Gửi POST request đến API
                const res = await fetch('/api/location/add', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify(data)
                });

                if (!res.ok) {
                    const errorText = await res.text();
                    console.error("Lỗi server:", errorText);
                    throw new Error("Lỗi khi thêm địa chỉ.");
                }

                const result = await res.json();
                alert(result.message);
                location.reload();
            } catch (err) {
                alert("Lỗi: " + err.message);
            }
        });
    </script>


</body>

</html>